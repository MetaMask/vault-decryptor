{
  "name": "metamask-state-log-explorer",
  "version": "1.0.0",
  "description": "A boilerplate for react-hyperscript using browserify with beefy and ES6.",
  "main": "index.js",
  "scripts": {
    "start": "beefy index.js:bundle.js --live --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify index.js -o bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "brfs"
    ]
  },
  "author": "Dan Finlay",
  "license": "ISC",
  "dependencies": {
    "browser-passworder": "^2.0.3",
    "ethereumjs-util": "^5.1.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hyperscript": "^3.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.5.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "xtend": "^4.0.1"
  },
  "keywords": [
    "react",
    "hyperscript",
    "beefy",
    "browserify",
    "boilerplate"
  ],
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MetaMask/state-log-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/MetaMask/state-log-explorer/issues"
  },
  "homepage": "https://github.com/MetaMask/state-log-explorer#readme"
}
